# python3

def binary_search(keys, query):
    """Binary search in a sorted array with duplicates.
    Returns the index of the first occurrence of query, or -1 if not found."""
    low = 0
    high = len(keys) - 1
    result = -1  # store potential match

    while low <= high:
        mid = low + (high - low) // 2
        if keys[mid] == query:
            result = mid
            # continue search in left half to find first occurrence
            high = mid - 1
        elif keys[mid] < query:
            low = mid + 1
        else:
            high = mid - 1

    return result


if __name__ == '__main__':
    n = int(input())
    input_keys = list(map(int, input().split()))
    assert len(input_keys) == n

    # Remove strict increasing assertion; allow duplicates
    assert all(input_keys[i] <= input_keys[i + 1] for i in range(len(input_keys) - 1))

    k = int(input())
    input_queries = list(map(int, input().split()))
    assert len(input_queries) == k

    print(" ".join(str(binary_search(input_keys, q)) for q in input_queries))
