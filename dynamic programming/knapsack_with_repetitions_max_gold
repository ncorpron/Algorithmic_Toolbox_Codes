from sys import stdin

#dynamic with repetitions
#time complexity: O(n*capacity)
#space complexity: O(capacity)

def maximum_gold_with_repetitions(capacity, weights):
    assert 1 <= capacity <= 10 ** 4
    assert 1 <= len(weights) <= 10 ** 3
    assert all(1 <= w <= 10 ** 5 for w in weights)

    value = [0] * (capacity + 1)
    for c in range(capacity + 1):
        for w in weights:
            if c >= w:
                value[c] = max(value[c], value[c - w] + w)
    return value[capacity]

if __name__ == '__main__':
    input_capacity, n, *input_weights = list(map(int, stdin.read().split()))
    assert len(input_weights) == n

    print(maximum_gold_with_repetitions(input_capacity, input_weights))
