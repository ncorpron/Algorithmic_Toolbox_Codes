def max_k(n):
    k = 0
    while (k * (k + 1)) // 2 <= n:
        k += 1
    return k - 1  # Because loop overshoots by 1

print(max_k(8))  # Output: 3 (sum of 1+2+3 = 6 â‰¤ 8)'''
