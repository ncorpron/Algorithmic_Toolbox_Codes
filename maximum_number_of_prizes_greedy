# python3


def compute_optimal_summands(n):
    assert 1 <= n <= 10 ** 9
    summands = []
    current_summand = 1

    while n > 0:
        if n - current_summand > current_summand:
            summands.append(current_summand)
            n -= current_summand
            current_summand += 1
        else:
            # Take the remaining n as the last summand
            summands.append(n)
            break

    return summands



if __name__ == '__main__':
    input_n = int(input())
    output_summands = compute_optimal_summands(input_n)
    print(len(output_summands))
    print(*output_summands)

