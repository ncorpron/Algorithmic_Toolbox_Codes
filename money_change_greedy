# python3


def money_change(money):
    assert 0 <= money <= 10 ** 3
    coins = [10, 5, 1]
    count = 0
    for coin in coins:
        while money >= coin:
            money -= coin
            count += 1
    return count






if __name__ == '__main__':
    input_money = int(input())
    print(money_change(input_money))
